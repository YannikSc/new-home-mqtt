{
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "constructor-super": 2,
        "for-direction": 2,
        "getter-return": 2,
        "no-async-promise-executor": 2,
        "no-case-declarations": 2,
        "no-class-assign": 2,
        "no-compare-neg-zero": 2,
        "no-cond-assign": 2,
        "no-const-assign": 2,
        "no-constant-condition": 2,
        "no-control-regex": 2,
        "no-debugger": 2,
        "no-delete-var": 2,
        "no-dupe-args": 2,
        "no-dupe-class-members": 2,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-empty": 2,
        "no-empty-character-class": 2,
        "no-empty-pattern": 2,
        "no-ex-assign": 2,
        "no-extra-boolean-cast": 2,
        "no-extra-semi": 2,
        "no-fallthrough": 2,
        "no-func-assign": 2,
        "no-global-assign": 2,
        "no-inner-declarations": 2,
        "no-invalid-regexp": 2,
        "no-irregular-whitespace": 2,
        "no-misleading-character-class": 2,
        "no-mixed-spaces-and-tabs": 2,
        "no-new-symbol": 2,
        "no-obj-calls": 2,
        "no-octal": 2,
        "no-prototype-builtins": 2,
        "no-redeclare": 2,
        "no-regex-spaces": 2,
        "no-self-assign": 2,
        "no-shadow-restricted-names": 2,
        "no-sparse-arrays": 2,
        "no-this-before-super": 2,
        "no-undef": 2,
        "no-unexpected-multiline": 2,
        "no-unreachable": 2,
        "no-unsafe-finally": 2,
        "no-unsafe-negation": 2,
        "no-unused-labels": 2,
        "no-unused-vars": 2,
        "no-useless-catch": 2,
        "no-useless-escape": 2,
        "no-with": 2,
        "require-yield": 2,
        "use-isnan": 2,
        "valid-typeof": 2,
        "array-bracket-spacing": 2,
        "array-callback-return": 2,
        "arrow-parens": 2,
        "arrow-spacing": 2,
        "block-scoped-var": 2,
        "brace-style": 2,
        "camelcase": 2,
        "comma-dangle": [
            2,
            "only-multiline"
        ],
        "array-bracket-newline": 2,
        "computed-property-spacing": 2,
        "complexity": 2,
        "comma-style": 2,
        "no-else-return": 2,
        "no-empty-function": 2,
        "no-extra-bind": 2,
        "no-implicit-coercion": 2,
        "no-implicit-globals": 2,
        "dot-location": 2,
        "dot-notation": 2,
        "no-import-assign": 2,
        "eqeqeq": 2,
        "eol-last": 2,
        "consistent-return": 2,
        "func-style": [
            2,
            "declaration"
        ],
        "function-paren-newline": 2,
        "no-invalid-this": 2,
        "no-label-var": 2,
        "no-lone-blocks": 2,
        "no-lonely-if": 2,
        "no-magic-numbers": [
            2,
            {
                "ignoreArrayIndexes": true
            }
        ],
        "grouped-accessor-pairs": 2,
        "guard-for-in": 2,
        "handle-callback-err": 2,
        "no-throw-literal": 2,
        "no-trailing-spaces": 2,
        "no-undef-init": 2,
        "no-undefined": 2,
        "no-unneeded-ternary": 2,
        "no-useless-call": 2,
        "no-use-before-define": 2,
        "no-useless-computed-key": 2,
        "no-useless-concat": 2,
        "no-useless-constructor": 2,
        "no-useless-rename": 2,
        "no-useless-return": 2,
        "no-var": 2,
        "object-curly-newline": 2,
        "object-shorthand": 2,
        "operator-assignment": 2,
        "operator-linebreak": 2,
        "padding-line-between-statements": 2,
        "prefer-const": 2,
        "prefer-exponentiation-operator": 2,
        "indent": 2,
        "prefer-promise-reject-errors": 2,
        "no-multiple-empty-lines": 2,
        "key-spacing": 2,
        "keyword-spacing": 2,
        "no-negated-condition": 2,
        "no-native-reassign": 2,
        "prefer-rest-params": 2,
        "prefer-spread": 2,
        "no-negated-in-lhs": 2,
        "linebreak-style": 2,
        "no-nested-ternary": 2,
        "prefer-template": 2,
        "no-new-func": 2,
        "no-new-object": 2,
        "no-new-require": 2,
        "lines-between-class-members": 2,
        "max-classes-per-file": 2,
        "max-depth": 2,
        "max-len": [
            2,
            120
        ],
        "max-lines": 2,
        "max-lines-per-function": 2,
        "max-nested-callbacks": 2,
        "max-params": 2,
        "max-statements": 2,
        "max-statements-per-line": 2,
        "semi": 2,
        "require-jsdoc": 2,
        "quotes": [
            2,
            "single",
            "avoid-escape"
        ],
        "multiline-comment-style": 2,
        "semi-style": 2,
        "sort-imports": [
            2,
            {
                "ignoreDeclarationSort": true,
                "ignoreMemberSort": true
            }
        ],
        "sort-keys": 2,
        "sort-vars": 2,
        "no-process-env": 2,
        "no-process-exit": 2,
        "new-parens": 2,
        "newline-before-return": 2,
        "no-array-constructor": 2,
        "strict": 2,
        "spaced-comment": 2,
        "no-return-assign": 2,
        "no-return-await": 2,
        "no-script-url": 2,
        "no-self-compare": 2,
        "no-console": 2,
        "no-constructor-return": 2,
        "no-tabs": 2,
        "no-shadow": 2
    },
    "env": {
        "browser": true,
        "es2021": true
    }
}
